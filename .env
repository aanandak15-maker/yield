# Environment Variables for Crop Yield Prediction API
# Copy and paste these into your Render service environment variables

ENVIRONMENT=production
PYTHON_VERSION=3.9.18

# Google Earth Engine Credentials
GEE_SERVICE_ACCOUNT=your_gee_service_account_email@test-project.iam.gserviceaccount.com
GEE_PRIVATE_KEY_PATH=/opt/render/project/src/gee_credentials.json

# Weather API Key
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Optional: Database URL if using external database
# DATABASE_URL=your_database_url_here
